<style>
	/* Full-screen overlay */
.loader-overlay {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.8); /* Dark semi-transparent */
  display: none;
  align-items: center;
  justify-content: center;
  z-index: 9999; /* Ensure it's above everything */
  transition: opacity 0.5s ease-in-out;
}

/* Loader container */
.loader-container {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  gap: 1.5rem;
}

/* Spinning loader */
.loader {
  width: 80px;
  height: 80px;
  border: 5px solid rgba(255, 255, 255, 0.3);
  border-top-color: var(--primary-color, #00ffcc);
  border-radius: 50%;
  animation: spin 1s ease-in-out infinite;
  position: relative;
}

.loader::before {
  content: '';
  position: absolute;
  top: -5px;
  left: -5px;
  right: -5px;
  bottom: -5px;
  border: 5px solid transparent;
  border-top-color: var(--secondary-color, #ff00cc);
  border-radius: 50%;
  animation: spin 2s linear infinite;
}

/* Loading text */
.loading-text {
  font-size: 1.5rem;
  color: white;
  font-weight: bold;
}

/* Spinning animation */
@keyframes spin {
  to {
    transform: rotate(360deg);
  }
}

/* Page content */
.content {
  opacity: 1;
  transition: opacity 0.5s ease-in-out;
}

/* Hide content initially */
.content.hidden {
  opacity: 0;
  display: none;
}

</style>
